  <body class="header_sticky">
    <% if notice %>
      <div class="alert alert-success">
        <%= notice %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close" style="all: unset; float: right;">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
    <% end %>

    <% if alert %>
      <div class="alert alert-danger">
        <%= alert %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close" style="all: unset; float: right;">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
    <% end %>
    <div class="boxed" data-name="<%= "#{controller_name}-#{action_name}" %>">
      <%= render 'shared/navbar' %>
      <%= yield %>
      <%= render 'shared/footer2'%>
    </div>

  </body>


